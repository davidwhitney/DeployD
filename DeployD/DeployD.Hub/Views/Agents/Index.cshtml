@{
    ViewBag.Title = "Index";
}
<link href="/Content/themes/base/jquery-ui.css" rel="stylesheet"/>
<link href="/Content/views/home.css" rel="stylesheet"/>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.0/jquery.min.js"></script>
<script src="/scripts/jquery-ui.min.js"></script>
  <script src="http://ajax.cdnjs.com/ajax/libs/json2/20110223/json2.js"></script>
  <script src="/Scripts/underscore.js"></script>
<script src="/Scripts/backbone.js"></script>
<script type="text/javascript" src="/Scripts/jsinq.js"></script>
<script src="/Scripts/Views/agents.js" type="text/javascript"></script>

<div id="app"></div>

<script id="agent-list-template" type="text/template">
    <ul>
        <%_.each(agents.models, function(agent) {%>
        <li>
            <%=agent.attributes.id%>
            <%if (!agent.attributes.approved) {%> <a href='#' class='approve-agent' data-id='<%=agent.attributes.id%>'>Approve</a> <%}%>
        </li>
        <%});%>
    </ul>
</script>